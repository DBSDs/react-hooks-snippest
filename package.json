{
  "name": "react-hooks-snippest",
  "displayName": "react-hooks-snippest/typescript",
	"description": "Code snippets for React-hooks in typescript",
	"version": "1.0.0",
  "galleryBanner": {
    "theme": "light",
    "color": "#FFFFFF"
  },
	"engines": {
		"vscode": "^1.66.0"
	},
  "icon": "images/react_ts.png",
	"publisher": "DBSDs",
	 "keywords": [
    "react",
    "snippets",
    "react-native",
    "hooks",
    "useStatus",
    "useEffect"
